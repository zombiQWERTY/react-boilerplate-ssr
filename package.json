{
  "name": "school",
  "version": "0.0.1",
  "description": "English School",
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "run-p dev:client:build dev:server:prepare dev:server:build dev:server:delay",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./static/server",
    "dev:client:build": "babel-node ./webpack/webpack-dev-server.js",
    "delay": "node ./node_modules/npm-delay 20000",
    "prod:start": "npm-run-all prod:client:build prod:server:build prod:server:start",
    "prod:client:build": "webpack --config ./webpack/webpack.config.client.production.babel.js --colors --display-error-details",
    "prod:server:build": "webpack --config ./webpack/webpack.config.server.production.babel.js --colors --display-error-details",
    "prod:server:start": "better-npm-run prod:server:start"
  },
  "betterScripts": {
    "prod:server:start": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "node ./src/server/start.babel.js"
    }
  },
  "author": "Roman Krasevych <krasevych@gmail.com>",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "styled components",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "global": "^4.3.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-form": "^0.9.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-immutablejs": "^0.0.8",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.2",
    "styled-components": "^1.4.3",
    "webpack-dev-middleware": "^1.10.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.14",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
